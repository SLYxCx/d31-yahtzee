{
  "expo": {
    "name": "d31-yahtzee",
    "slug": "d31-yahtzee",
    "version": "1.0.0",
    "scheme": "d31-yahtzee-scheme",
    "orientation": "portrait",
    "icon": "./assets/d31icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#922018"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#922018"
      },
      "package": "com.d31yahtzee"
    },
    "web": {
      "favicon": "./assets/d31icon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
